{
  "prettier.requireConfig": true,
  "tslint.enable": true,
  "tslint.autoFixOnSave": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "Markdown",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "yaml.schemas": {
    "kubernetes": "/*.yaml"
  },
  "yaml.trace.server": "verbose",
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod"
      ],
      "trace.server": "verbose",
      "filetypes": [
        "go"
      ]
    }
  }
}
